{
  "modules": {
    "wallet-connect": {
      "path": "/wallet",
      "component": "WalletConnect.vue",
      "requiredServices": ["multi-sig-manager", "rpc-manager"],
      "permissions": ["read", "connect"]
    },
    "trading-params": {
      "path": "/trading",
      "component": "TradingParameters.vue",
      "requiredServices": ["position-sizer", "risk-assessment", "strategy-selector"],
      "permissions": ["read", "write", "configure"]
    },
    "optimize-simulate": {
      "path": "/optimize",
      "component": "StrategySimulator.vue",
      "requiredServices": ["genetic-optimizer", "strategy-optimizer", "performance-monitor"],
      "permissions": ["read", "write", "execute"]
    },
    "deploy-live": {
      "path": "/deploy",
      "component": "DeployLive.vue",
      "requiredServices": ["main-orchestrator", "health-monitor", "zero-downtime-manager"],
      "permissions": ["deploy", "execute"]
    },
    "continued-optimization": {
      "path": "/optimization",
      "component": "ContinuedOptimization.vue",
      "requiredServices": ["strategicAI", "tacticalAI", "pattern-analyzer"],
      "permissions": ["read", "configure"]
    },
    "live-monitoring": {
      "path": "/monitoring",
      "component": "LiveMonitoring.vue",
      "requiredServices": ["cross-chain-scanner", "alert-manager", "metrics-server"],
      "permissions": ["read", "monitor"]
    },
    "profit-withdrawal": {
      "path": "/profits",
      "component": "ProfitWithdrawal.vue",
      "requiredServices": ["key-rotation", "profit-calculator", "gasless-manager"],
      "permissions": ["read", "withdraw"]
    },
    "ai-terminal": {
      "path": "/ai-terminal",
      "component": "AITerminal.vue",
      "requiredServices": ["strategicAI", "tacticalAI", "pattern-recognition"],
      "permissions": ["read", "analyze"]
    }
  }
}
